{
  "name": "vue-plotly",
  "version": "1.1.0",
  "private": false,
  "author": "Nicolas Descoins",
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build": "vue-cli-service build --name vue-plotly --entry ./src/index.js --target lib --skip-plugins prettier",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:doc": "vue-cli-service build --entry ./example/main.js --dest docs --mode development",
    "prepublishOnly": "npm run lint && npm run build && npm run build:doc",
    "test:coverage": "vue-cli-service test:unit --coverage --verbose && codecov"
  },
  "main": "dist/vue-plotly.umd.js",
  "module": "dist/vue-plotly.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "core-js": "^3.25.3",
    "plotly.js": "^2.15.1",
    "vue": "^2.7.10",
    "vue-resize-directive": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "@vuedoc/md": "^1.6.0",
    "assert": "^2.0.0",
    "babel-jest": "^27.0.6",
    "bootstrap": "^4.6.0",
    "bubleify": "^2.0.0",
    "codecov": "^3.8.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "font-awesome": "^4.7.0",
    "ify-loader": "^1.1.0",
    "jest": "^27.1.0",
    "jest-canvas-mock": "^2.3.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "prettier": "^2.4.1",
    "stream-browserify": "^3.0.0",
    "vue-draggable-resizable": "^2.3.0",
    "vue-json-editor": "^1.4.3",
    "vue-server-renderer": "^2.7.10",
    "vue-template-compiler": "^2.7.10"
  },
  "from": "https://github.com/ndpirat/vue-plotly",
  "homepage": "https://github.com/ndpirat/vue-plotly",
  "keywords": [
    "vue",
    "component"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndpirat/vue-plotly"
  }
}
